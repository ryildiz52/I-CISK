              <Size>28</Size>
            </Graphic>
          </PointSymbolizer>
          <Filter>
            <ogc:PropertyIsEqualTo>
              <ogc:PropertyName>fid</ogc:PropertyName>
              <ogc:Literal>2</ogc:Literal>
            </ogc:PropertyIsEqualTo>
          </Filter>
        </Rule>
